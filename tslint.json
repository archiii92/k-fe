{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-airbnb"
    ],
    "rules": {
      "interface-name" : [true, "never-prefix"],
      "member-access": [true, "no-public"],
      "jsx-no-multiline-js": [false],
      "max-line-length": [true, 150]
    }
}